  "license": "MIT",
  "dependencies": {
    "@ethersproject/abi": "^5.1.3",
    "@ethersproject/providers": "^5.1.3",
    "ccxt": "^2.1.63",
    "dotenv": "^16.0.3",
    "ethers": "^5.0.0"
  },
  "devDependencies": {
    "pkg": "^5.8.0",
    "@typechain/ethers-v5": "workspace:^10.1.1",
    "@types/bn.js": "^5.1.0",
    "ts-node": "^10.7.0",
    "tsc": "^2.0.4",
    "typechain": "workspace:^8.1.1",
    "typescript": "^4.6"
  },
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'abi/*.json'",
    "start": "ts-node ./src/index.ts",
    "typecheck": "yarn generate-types && tsc --noEmit",
    "generate-linux-binary": "tsc ./src/index.ts && npx pkg -t node16-linux src/index.js -o pricewatch"
  }
}